@import 'nib'

body
	background #333

#artContainer
	visibility hidden
	background #ddd
	width 1000px
	margin auto

.box
	float left
	margin 2px
	border-radius 5px

#overlay
	z-index 100
	position fixed
	left 0
	top 0
	width 100%
	height 100%
	display none

#overlayBackground
	width 100%
	height 100%
	background #000
	opacity 0.8
	position absolute

#overlayPic
	position absolute
	border-radius 10px
